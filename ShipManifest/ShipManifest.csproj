copy /Y "$(TargetDir)CLSInterfaces.dll" "%25KSP_DIR%25\GameData\ShipManifest\Plugins\"
copy /Y "$(TargetDir)DFInterface.dll" "%25KSP_DIR%25\GameData\ShipManifest\Plugins\"

echo distributing files...
copy /Y "$(TargetPath)" "$(ProjectDir)Distribution\GameData\ShipManifest\Plugins\"
copy /Y "$(TargetDir)DFInterface.dll" "$(ProjectDir)Distribution\GameData\ShipManifest\Plugins\"
copy /Y "$(TargetDir)CLSInterfaces.dll" "$(ProjectDir)Distribution\GameData\ShipManifest\Plugins\"

echo building .mdb file...
cd "$(TargetDir)"
call "%25MONO_EXE_DIR%25\cli.bat"  "%25PDB2MDB_EXE_DIR%25\pdb2mdb.exe" ShipManifest.dll

copy /Y "$(TargetDir)ShipManifest.dll.mdb" "%25KSP_DIR%25\GameData\ShipManifest\Plugins\"

echo packaging files...
if exist "E:\KSP\Mods\PJS\Releases\ShipManifest\Build\ShipManifest*.zip" del "E:\KSP\Mods\PJS\Releases\ShipManifest\Build\ShipManifest*.zip"
call "%25ZA_DIR%25\7za.exe" a -tzip -r  "%25DIST_DIR%25\ShipManifest.@(VersionNumber)_%25DATE:~4,2%25%25DATE:~7,2%25%25DATE:~10,4%25.zip" "$(ProjectDir)Distribution\*.*"
echo Build complete!</PostBuildEvent>
